@model List<System.Security.Claims.Claim>
@{
    ViewData["Title"] = "Claims";
    Layout = "~/Views/Admin/_AdminLayout.cshtml";
}

<h1>Claims</h1>
<hr />
<table class="table accept-policy table-striped table-responsive">
    <tr>
        <th>Kim</th>
        <th>Dağıtıcı</th>
        <th>Adı</th>
        <th>Değer</th>
        @foreach (System.Security.Claims.Claim item in Model)
        {
        <tr>
            <td>@item.Subject.Name</td>
            <td>@item.Issuer</td>
            <td>@item.Type</td>
            <td>@item.Value</td>
        </tr>
        }
    </tr>

</table>
